{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src"
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src"
    }
  },
  "monorepo": true,
  "root": ".",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.json",
    "webpack": false
  }
}